<Window x:Class="ProjektSemestralny.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjektSemestralny"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Window.Resources>
        <local:HotelDataSet x:Key="hotelDataSet"/>
        <CollectionViewSource x:Key="klienciViewSource" Source="{Binding Klienci, Source={StaticResource hotelDataSet}}"/>
    </Window.Resources>
    <Grid DataContext="{StaticResource klienciViewSource}">
        <Frame x:Name="Frame1" Content="Frame" HorizontalAlignment="Left" Height="366" Margin="39,30,0,0" VerticalAlignment="Top" Width="701"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="273,140,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <DataGrid HorizontalAlignment="Left" Height="100" Margin="273,279,0,0" VerticalAlignment="Top" Width="309"/>
    </Grid>
</Window>
