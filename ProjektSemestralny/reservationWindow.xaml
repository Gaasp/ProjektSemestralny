        xmlns:local="clr-namespace:ProjektSemestralny"
        mc:Ignorable="d"
        Title="reservationWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="189" Margin="125,205,0,0" VerticalAlignment="Top" Width="523"/>
        <Button x:Name="load_button" Content="Załaduj" HorizontalAlignment="Left" Margin="336,59,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <TextBox x:Name="search_TextBox" HorizontalAlignment="Left" Height="23" Margin="309,164,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="search_TextBox_TextChanged"/>
