<Window x:Class="ProjektSemestralny.reservationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjektSemestralny"
        mc:Ignorable="d"
        Title="reservationWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Assets/background.jpg"/>
        </Grid.Background>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="191" Margin="10,207,0,0" VerticalAlignment="Top" Width="772"/>
        <Button x:Name="load_button" Content="Załaduj" HorizontalAlignment="Left" Foreground="White" Background="Gray" Margin="339,150,0,0" VerticalAlignment="Top" Width="82" Click="Button_Click_2" Height="34"/>
        <TextBox x:Name="search_TextBox" HorizontalAlignment="Left" Height="38" Margin="319,71,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="search_TextBox_TextChanged" IsEnabled="False"/>
        <Button Content="Cofnij" HorizontalAlignment="Left" Margin="36,41,0,0"  Foreground="White" Background="Gray" VerticalAlignment="Top" Width="85" Click="Button_Click_3" Height="33" RenderTransformOrigin="0.321,0.92"/>
    </Grid>
</Window>
